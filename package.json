{
  "name": "lambda-edge-locale",
  "version": "0.1.0",
  "description": "Lambda Edge (CloudFront CDN) function that allows caching based on language or locale",
  "main": "fetchlocale.js",
  "scripts": {
    "setup": "npm install",
    "pretest": "npm run build",
    "test": "if ! npm list | grep -q chai; then npm install --development; fi && npm run tslint && nyc -x 'index*' -x 'test/*' mocha",
    "test-only": "mocha",
    "build": "tsc",
    "tslint": "node_modules/tslint/bin/tslint -c tslint.json '*.ts'",
    "deploy": "tsc && rm -rf node_modules && npm install --production",
    "createzip": "npm run deploy && zip -r -X '../lambda-edge-locale.zip' *"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.6.2",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.4"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "accept-language-parser": "^1.5.0",
    "query-string": "^6.8.1"
  }
}
